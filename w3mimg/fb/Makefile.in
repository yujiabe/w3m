#
# w3mimg/fb/Makefile
#
#
@SET_MAKE@
SHELL=@SHELL@
srcdir = @srcdir@
top_srcdir = @top_srcdir@
VPATH = $(srcdir):.
RM=rm

IMGCFLAGS=@IMGFBCFLAGS@
OBJS=fb.o fb_img.o 

all: fb_w3mimg.o fb.o fb_img.o

fb_w3mimg.o: fb_w3mimg.c
	$(CC) $(CFLAGS) $(IMGCFLAGS) -I$(top_srcdir) -c $<

fb.o: fb.c
	$(CC) $(CFLAGS) $(IMGCFLAGS) -I$(top_srcdir) -c $<

fb_img.o: fb_img.c fb_gdkpixbuf.c fb_imlib2.c
	$(CC) $(CFLAGS) $(IMGCFLAGS) -I$(top_srcdir) -c $<

clean:
	@-$(RM) -f *.o

